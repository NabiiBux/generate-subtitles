extends layout

block content
    style.
        p {
            margin: 2px auto;
        }


    div
        h1 Admin
        br

        h2 Job Processes
        each val, key in jobProcesses
            p Process Number: #{key}
            br
            if val
                each val2, key2 in val
                    p #{key2}: #{val2}

        br
        h2 New Queue Items (#{newQueue && newQueue.length})
        each item, index in newQueue
            p Place In Queue: #{index}
            each val, key in item
                p #{key}: #{val}
                br
        br

        br
        h2 Transcription/Download Processes
        each item in transcriptions
          each val, key in item
              p #{key}: #{val}
          br
        br

        h2 Websocket Connections
        each item in websocketItems
            each val, key in item
                p #{key}: #{val}
            br



